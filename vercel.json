{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/public/$1"
    }
  ],
  "functions": {
    "api/analyze.js": {
      "maxDuration": 60
    },
    "api/generate.js": {
      "maxDuration": 60
    }
  }
}
